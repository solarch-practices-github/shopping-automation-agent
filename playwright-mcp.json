{
  "browser": {
    "isolated": false,
    "launchOptions": {
      "channel": "chrome",
      "headless": false,
      "executablePath": "/usr/bin/google-chrome"
    },
    "contextOptions": {
      "viewport": {
        "width": 1200,
        "height": 1100
      },
      "timeout": 15000
    }
  },
  "interaction": {
    "maxActions": 50,
    "maxNavigationDepth": 10,
    "requireVisibleElement": true,
    "clickTimeoutMs": 5000,
    "typeDelayMs": 30
  },
  "outputMode": "file",
  "outputDir": ".playwright-mcp"
}